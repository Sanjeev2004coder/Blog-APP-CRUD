{
  "version": 2,
  "builds": [
    {
      "src": "blog_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "blog/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/blog/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "blog_project/wsgi.py"
    }
  ]
}